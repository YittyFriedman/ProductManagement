
@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Add Product";
}
<script src="~/Scripts/UserDefined/Add.js"></script>
<script src="~/Scripts/jquery-ui.js"></script>
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<h2 style="padding-top:20px">@ViewData["Title"]</h2>

<div class="container form-group">
    @*Product Description*@
    <div class="row row-bordered" style="padding-top:25px">
        <div class="col-sm-8 ">
            <h5 class="lead" style="margin-bottom:2px">Product Description</h5>
        </div>
        <div class="col-sm-2 ">
            <button class="btn btn-primary" hidden id="btnRefactorAMItem">Refactor AM Product</button>

        </div>
    </div>



    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:20px" class="col-sm-3"> <label class="col-form-label required">Product Id Prefix:</label></div>
        <div style="margin-top:20px" class="col-sm-3 " id="divProductIdPrefix"><input title="1-10 characters based on product name" style="text-align: center; text-transform: uppercase" type="text" placeholder="" required class="form-control" maxlength="10" id="txtPrdIdPrfx" /></div>
        <div style="margin-top:20px" class="col-sm-5 " hidden id="divPrdIdError"><span class="help-block">Product Id Prefix must start with an alpha character.</span></div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"> <label class="col-form-label required">Base Item Description:</label></div>
        <div style="margin-top:10px" class="col-sm-3" id="divBaseItemDesc"><input title="Title case, no abbreviations, no variations" style="text-align:center" type="text" required class="form-control" id="txtBsItmDesc" /></div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"> <label class="col-form-label">Manufacturer:</label></div>
        <div style="margin-top:10px" class="col-sm-3" id="divSupplier">
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnSupplier" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblSupplier" class="spanOverflow">Select one</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmSupplier" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"> <label class="col-form-label required">Brand:</label></div>
        <div style="margin-top:10px" class="col-sm-3" id="divBrand">
            <input type="text" style="text-align:center" class="form-control" id="txtBrand" />
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"><label class="col-form-label required">Category 1:</label></div>
        <div style="margin-top:10px" class="col-sm-3" id="divClass1">
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnClass1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblClass1" class="spanOverflow">Select one</span>
                </button>
                <ul class="dropdown-menu" id="ddmClass1" style="min-width: 100%;" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"><label class="col-form-label">Category 2:</label>&nbsp;(Optional)</div>
        <div style="margin-top:10px" class="col-sm-3" id="divClass2">
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnClass2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblClass2" class="spanOverflow">Select one</span>
                </button>
                <ul class="dropdown-menu" id="ddmClass2" style="min-width: 100%;" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"><label class="col-form-label">Category 3:</label>&nbsp;(Optional)</div>
        <div style="margin-top:10px" class="col-sm-3" id="divClass3">
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnClass3" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblClass3" class="spanOverflow">Select one</span>
                </button>
                <ul class="dropdown-menu" id="ddmClass3" style="min-width: 100%;" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"><label class="col-form-label required">Inventory Type:</label></div>
        <div style="margin-top:10px" class="col-sm-3" id="divType">
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnType" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblType" class="spanOverflow">Select one</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmType" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>



    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"><label class="col-form-label ">Vendor Id:</label></div>
        <div style="margin-top:10px" class="col-sm-3" id="divVendorId">
            <input type="text" class="form-control txtVendorId" style="text-align:center" id="txtVendorId" />
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3"><label class="col-form-label ">Vendor Part No:</label></div>
        <div style="margin-top:10px" class="col-sm-3" id="divVendorPartNo">
            <input type="text" class="form-control" style="text-align:center" id="txtVendorPartNo" maxlength="20" />
        </div>
    </div>
    @*Product Variants*@
    <div class="row" style="padding-top:25px">
        <div class="col-sm-12 row-bordered">
            <h5 class="lead" style="margin-bottom:2px">Product Variants</h5>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:20px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChooseColor" value="hasColor" />
            <label class="col-form-label" id="lblColorLabel">1. Color</label>
        </div>
        <div style="margin-top:20px" class="col-sm-3" id="divChooseColor" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" id="ddmBtnColor" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblColor" class="spanOverflow">Select Available Colors</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmColor" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChooseSize" value="hasSize" />
            <label class="col-form-label " id="lblSizeLabel">2. Size</label>
        </div>
        <div style="margin-top:10px" class="col-sm-3" id="divChooseSize" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnSize" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblSize" class="spanOverflow">Select Available Sizes</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmSize" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChooseOrientation" value="hasOrntn" />
            <label class="col-form-label " id="lblOrientationLabel">3. Orientation</label>
        </div>
        <div style="margin-top:10px" class="col-sm-3" id="divChooseOrientation" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnOrientation" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblOrientation" class="spanOverflow">Select Available Orientations</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmOrientation" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChooseFlavor" value="hasFlavor" />
            <label class="col-form-label " id="lblFlavorLabel">4. Flavor</label>
        </div>
        <div style="margin-top:10px" class="col-sm-3" id="divChooseFlavor" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnFlavor" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblFlavor" class="spanOverflow">Select Available Flavors</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmFlavor" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChoosePackaging" value="hasPckg" />
            <label class="col-form-label " id="lblPackagingLabel">5. Packaging</label>
        </div>
        <div style="margin-top:10px" class="col-sm-3" id="divChoosePackaging" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnPackaging" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblPackaging" class="spanOverflow">Select Available Packaging</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmPackaging" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChooseUOM" value="hasUOM" />
            <label class="col-form-label " id="lblUOMLabel">6. Unit of Measure</label>
        </div>
        <div style="margin-top:10px" class="col-sm-3" id="divChooseUOM" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnUOM" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblUOM" class="spanOverflow">Select Available UOM</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmUOM" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChooseStrength" value="hasStrength" />
            <label class="col-form-label " id="lblStrengthLabel">7. Strength</label>
        </div>
        <div style="margin-top:10px" class="col-sm-3" id="divChooseStrength" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnStrength" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblStrength" class="spanOverflow">Select Available Strength</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmStrength" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChooseGender" value="hasGender" />
            <label class="col-form-label " id="lblGenderLabel">8. Gender</label>
        </div>
        <div style="margin-top:10px" class="col-sm-3" id="divChooseGender" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnGender" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblGender" class="spanOverflow">Select Available Gender</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmGender" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-3">
            <input type="checkbox" class="form-check" style="display:inline" id="ckChooseOther" value="hasOther" />
            <label class="col-form-label " id="lblOtherLabel">9. Other</label>
        </div>
        <div style="margin-top:10px" class="col-sm-3" id="divChooseOther" hidden>
            <div class="dropdown">
                <button class="form-control dropdown-toggle" type="button" id="ddmBtnOther" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="lblOther" class="spanOverflow">Select Available Option</span>
                </button>
                <ul class="dropdown-menu dropdown-menu-regular" id="ddmOther" aria-labelledby="dropdownMenuButton">
                </ul>
            </div>
        </div>
    </div>

    @*Product Summary*@
    <div class="row" style="padding-top:25px">
        <div class="col-sm-12 row-bordered">
            <h5 class="lead" style="margin-bottom:2px">Product Summary</h5>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:20px" class="col-sm-7">
            <input type="checkbox" class="form-check" style="display:inline" id="ckOneTime" value="oneTime" />
            <span>Check if this is a "one time" item</span>
        </div>

    </div>
    <div class="row">
        <div class="col-sm-1"></div>
        <div style="margin-top:10px" class="col-sm-7">
            <input type="checkbox" class="form-check" style="display:inline" id="ckManual" value="manual" />
            <span>Check to manually enter Product ID's on variations</span>
        </div>

    </div>
    <div class="row" id="divLoadProductsButton">
        <div class="col-sm-1"></div>
        <div style="margin-top:30px" class="col-sm-7">
            <button class="btn btn-primary" type="submit" id="btnLoadProductTable">Load Product Table</button>
        </div>


    </div>

    <div class="row" id="divLoadingProducts" hidden>
        <div class="col-sm-1"></div>
        <div style="margin-top:30px" class="col-sm-7 ">
            <p class="text-muted">LOADING PRODUCTS TABLE.....</p>
            <div class="progress progress-striped active">
                <div class="progress-bar" style="width: 25%"></div>
            </div>
        </div>

    </div>
    <div class="row" id="divProductGrid" hidden>
        <div style="margin-top:30px" class="col-sm-12">
            <table id="tblProducts" class="table table-striped table-hover">
                <tr>
                    <th width="12%">Product Id</th>
                    <th width="15%">Description</th>
                    <th>
                        <div style="float:left;width:75%;"><span class="required">UPC</span></div>
                        <div style="float:right;width:25%;"></div>
                    </th>
                    <th>No UPC</th>
                    @*<th>
                            <div style="float:left;width:75%;">Vendor Id</div>
                            <div style="float:right;width:25%;"><img id="imgCopyVendorId" width="15" height="15" title="Copy value for all Vendor Id's" src="~/Images/copy icon.png" /></div>
                        </th>
                        <th>
                            <div style="float:left;width:75%;">Vendor Part No</div>
                            <div style="float:right;width:25%;"><img id="imgCopyVendorId" width="15" height="15" title="Copy value for all Vendor Id's" src="~/Images/copy icon.png" /></div>
                        </th>*@
                    <th width="10%">
                        <div style="float:left;width:75%;"><span class="required">Cost</span></div>
                        <div style="float:right;width:25%;"><img id="imgCopyCost" width="15" height="15" title="Copy value for all Cost fields" src="~/Images/copy icon.png" /></div>
                    </th>
                    <th width="10%">
                        <div style="float:left;width:75%;"><span class="required">Weight</span></div>
                        <div style="float:right;width:25%;"><img id="imgCopyWeight" width="15" height="15" title="Copy value for all Weight fields" src="~/Images/copy icon.png" /></div>
                    </th>
                    <th width="10%">
                        <div style="float:left;width:75%;"><span class="required">Length</span></div>
                        <div style="float:right;width:25%;"><img id="imgCopyLength" width="15" height="15" title="Copy value for all Length fields" src="~/Images/copy icon.png" /></div>
                    </th>
                    <th width="10%">
                        <div style="float:left;width:75%;"><span class="required">Width</span></div>
                        <div style="float:right;width:25%;"><img id="imgCopyWidth" width="15" height="15" title="Copy value for all Width fields" src="~/Images/copy icon.png" /></div>
                    </th>
                    <th width="10%">
                        <div style="float:left;width:75%;"><span class="required">Height</span></div>
                        <div style="float:right;width:25%;"><img id="imgCopyHeight" width="15" height="15" title="Copy value for all Height fields" src="~/Images/copy icon.png" /></div>
                    </th>
                </tr>

            </table>
        </div>

    </div>
    <div class="row" id="divAssociateProductsBtn" hidden>
        <div class="col-sm-1"></div>
        <div style="margin-top:30px" class="col-sm-7">
            <button class="btn btn-primary" id="btnAssociateProducts">Associate Products</button>
        </div>
    </div>
    <div class="row" id="divSaveProductsButton" hidden>
        <div class="col-sm-1"></div>
        <div style="margin-top:30px" class="col-sm-7">
            <button class="btn btn-primary" type="submit" id="btnSaveProducts">Save New Products</button>
        </div>
    </div>
</div>


<!-- Modal -->
<div class="modal fade" id="addModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalCenterTitle">Add New <span id="spnModalTitle"></span></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row" id="divModelName">
                    <div class="col-sm-5">     <label id="lblModelName"></label></div>

                    <div class="col-sm-5">
                        <input type="text" id="txtModelName" required class="form-control" />
                    </div>
                </div>
                <div class="row" id="divModelNameUOM" hidden>
                    <div class="col-sm-5">
                        <br />   <label id="lblModelNameUOM">UOM:</label>
                    </div>
                    <div class="col-sm-5">
                        <br />
                        <div class="input-group">
                            <input type="number" min="0" class="form-control txtModalUOM" id="txtModalUOM" placeholder="Factor" value="" />
                            <div class="input-group-btn uomDDBtn">
                                <button type="button" class="dropdown-toggle form-control" style="text-align:right" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span id="spnModalUOMSelected" data-value="">
                                        <span class="caret"></span>
                                    </span>
                                </button>
                                <ul class="dropdown-menu dropdown-menu-regular dropdown-menu dropdown-menu-regular-right ddmModalUOM"></ul>
                            </div>
                        </div>

                    </div>
                </div>

                <div class="row" id="divModelCode">
                    <div class="col-sm-5">
                        <br />   <label id="lblModelCode"></label>
                    </div>
                    <div class="col-sm-5">
                        <br />  <input type="text" id="txtModelCode" maxlength="3" style="text-transform: uppercase" class="form-control" placeholder="Enter 3 digit Code" />
                    </div>
                </div>

                <div class="row" id="divModelTitle" hidden>
                    <div class="col-sm-5">
                        <br />   <label id="lblModelTitle"></label>
                    </div>
                    <div class="col-sm-5">
                        <br />  @*<input type="text" id="txtModelTitle" class="form-control" />*@
                        <div class="dropdown">
                            <button class="form-control dropdown-toggle" type="button" id="ddmBtnModalTitle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span id="lblTitle"  class="spanOverflow">Select Available Title</span>
                            </button>
                            <ul class="dropdown-menu dropdown-menu-regular" id="ddmTitle" aria-labelledby="dropdownMenuButton">

                            </ul>
                        </div>
                    </div>
                </div>
                <br />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary saveAddNew">Save changes</button>
            </div>
        </div>

    </div>
</div>


<div class="modal fade" id="idAMItemModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalCenterTitle">Refactor AM Product <span id="spnModalTitle"></span></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="divScreen1">
                    <div class="row" id="divModelName">
                        <div class="col-sm-3">AM Item No:</div>

                        <div class="col-sm-6">
                            <input type="text" id="txtModelAMProduct" required class="form-control" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3"></div>
                        <div style="padding-top:10px" class="col-sm-6">
                            <span id="spnItemNotFound" hidden class="alert-info">Item was not found in AM.</span>
                        </div>
                    </div>
                </div>
                <div id="divScreen2" hidden>
                    <div class="row">
                        <div class="col-sm-12"><h4 class="" id="spnItemInfo"></h4></div>
                    </div>
                    <div class="row" style="padding-top:20px">
                        @* <div class="col-sm-1"></div>
        <button class="btn btn-primary small">Product Family Lookup</button>*@
                        <div class="col-sm-8">  <strong> <span>Associate sibling items:</span></strong></div>
                        <div class="col-sm-3"><button id="btnAddSibling" class="btn btn-primary btn-sm">&#43; Add Sibling</button></div>
                    </div>
                    <div class="row" style="padding-top:10px">
                        <div class="col-sm-12" id="divSiblingTblContainer" style="max-height:350px; overflow-y:auto">
                            <table  id="tblSiblingItems" class="table table-striped table-hover">
                                <tr>
                                    <th width="18%"></th>
                                    <th width="30%">Item No</th>
                                    <th>Description</th>
